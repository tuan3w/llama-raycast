{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 16",
  "include": [
    "src/**/*",
    "raycast-env.d.ts"
  ],
  "compilerOptions": {
    "lib": [
      "es2021"
    ],
    "module": "ESNext",
    "target": "ESNext",
    "strict": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "resolveJsonModule": true,
    "moduleResolution": "nodenext",
    "allowJs": true,
  }
}